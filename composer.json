{
    "name": "ringierimu/image-service-php",
    "description": "Ringier SA Image Service PHP integration helper",
    "keywords": [],
    "homepage": "https://github.com/RingierIMU/image-service-php",
    "license": "proprietary",
    "authors": [
        {
            "name": "Matthew Nessworthy",
            "email": "matthew.nessworthy@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.3",
        "nullpunkt/lz-string-php": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RingierIMU\\ImageService\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "fix-code-style": [
            "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no src tests"
        ],
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
